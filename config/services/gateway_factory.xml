<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" autoconfigure="false" autowire="false"/>

        <service id="eightlines.sylius_simpay_plugin.gateway_factory" class="Payum\Core\Bridge\Symfony\Builder\GatewayFactoryBuilder">
            <argument>EightLines\SyliusSimPayPlugin\SimPayGatewayFactory</argument>
            <tag name="payum.gateway_factory_builder" factory="simpay" />
        </service>
    </services>
</container>
